#Nodes
node1:
	ssh root@$(NODE1)
node1-updateDocker:
	ssh root@$(NODE1) "docker pull $(TEACHER_IMAGE) && docker pull $(STUDENT_IMAGE) && docker system prune -f"
node2:
	ssh root@$(NODE2)
node2-updateDocker:
	ssh root@$(NODE2) "docker pull $(TEACHER_IMAGE) && docker pull $(STUDENT_IMAGE) && docker system prune -f"
